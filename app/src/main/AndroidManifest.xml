<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.predictcovid">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PredictCOVID">
        <activity android:name=".ui.country.CovidCountryDetail"></activity>
        <activity android:name=".DataActivity" />
        <activity android:name=".CalculatorSummary" />
        <activity
            android:name=".TrackerActivity"
            android:label="@string/title_activity_tracker" />
        <activity android:name=".GraphActivity" />
        <activity android:name=".CalculatorActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>